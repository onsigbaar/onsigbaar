{
  "extends"      : "eslint:recommended",
  "plugins"      : [
    "angular"
  ],
  "rules"        : {
    "angular/no-service-method": 0
  },
  "env"          : {
    "es6"    : true,
    "browser": true,
    "jasmine": true
  },
  "ecmaFeatures" : {
    "modules": true
  },
  "globals"      : {
    "angular": true,
    "module" : true,
    "inject" : true,
    "require": true,
    "console": true
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
